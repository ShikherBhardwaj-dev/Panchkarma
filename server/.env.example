# Copy this file to .env and fill in values for local development
# Twilio credentials (either name variants are accepted)
TWILIO_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_TOKEN=your_twilio_auth_token
# or
# TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# TWILIO_AUTH_TOKEN=your_twilio_auth_token

# WhatsApp from number (twilio sandbox or production): accept either with or without 'whatsapp:' prefix
TWILIO_WHATSAPP_FROM=whatsapp:+14155238886

# Optional: webhook URL for Twilio delivery status callbacks
TWILIO_STATUS_CALLBACK_URL=https://<your-ngrok-id>.ngrok.io/admin/twilio-status

# Server port
PORT=5000

# MongoDB connection string
MONGO_URI=mongodb://localhost:27017/panchkarma
